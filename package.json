    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.16.2",
    "express-validator": "^6.0.1",
    "graphql-resolvers": "^0.4.2",
    "lodash": "^4.17.13",
    "lusca": "^1.6.1",
    "pug": "^2.0.4",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "winston": "^3.2.0",
    "winston-elasticsearch": "0.8.1"
  },
  "devDependencies": {
    "@types/async": "^3.0.0",
    "@types/bcrypt-nodejs": "^0.0.30",
